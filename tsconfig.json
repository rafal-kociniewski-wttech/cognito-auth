{
  "extends": "@tsconfig/node16/tsconfig.json",
  "include": [
    "src",
  ],
  "ts-node": {
    "esm": true
  },
  "lib": ["esnext"],
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
  }
}
